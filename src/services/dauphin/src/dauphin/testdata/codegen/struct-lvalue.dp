struct stest {
    A: boolean,
    B: boolean
};

proc assign(out _A, _A);
inline ":=" assign left 14;
proc assert(boolean,boolean);

x := stest { A: false, B: false };
assert(x.A,false);
assert(x.B,false);
x.A := true;
assert(x.A,true);
assert(x.B,false);
x.A := false;
x.B := true;
assert(x.A,false);
assert(x.B,true);
