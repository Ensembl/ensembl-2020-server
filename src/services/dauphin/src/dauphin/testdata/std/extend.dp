import "lib:std";
use "std";

x := [[1,2][] (+) [3,4][]];
assert(x==[1,2,3,4],true);

x := [[1,2] (+) [3,4]];
assert(x==[[1,2],[3,4]],true);

x := [1,2];
print_vec(push(x,3));
x := push(x,3);
print_vec(x);
assert(x==[1,2,3],true);

x := [1,2];
x := append(x,[3,4]);
assert(x==[1,2,3,4],true);

x := [[1,2],[3,4]];
x := [x,x];
print_vec(x);
assert(x==[[[1,2],[3,4]],[[1,2],[3,4]]],true);

y := [[6,6,6],[7]];

x := push(x,y);
assert(x==[[[1,2],[3,4]],[[1,2],[3,4]],[[6,6,6],[7]]],true);
