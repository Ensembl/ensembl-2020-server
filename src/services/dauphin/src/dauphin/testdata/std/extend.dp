import "search:std";
use "std";

x := [[1,2][] (+) [3,4][]];
assert(x==[1,2,3,4],true);

x := [[1,2] (+) [3,4]];
assert(x==[[1,2],[3,4]],true);

x := [1,2];
print_vec([x[] (+) 3]);
x := [x[] (+) 3]; // push!
print_vec(x);
assert(x==[1,2,3],true);

x := [1,2];
x := [x[] (+) [3,4][]]; // multi-push
assert(x==[1,2,3,4],true);

x := [[1,2],[3,4]];
x := [x,x];
print_vec(x);
assert(x==[[[1,2],[3,4]],[[1,2],[3,4]]],true);

y := [[6,6,6],[7]];

x := [x[] (+) y];
assert(x==[[[1,2],[3,4]],[[1,2],[3,4]],[[6,6,6],[7]]],true);
